  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GSDLabs-eu/nuker.git"
  },
  "bugs": {
    "url": "https://github.com/GSDLabs-eu/nuker/issues"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "bin": "./bin/loadtest.js",
  "main": "index.js",
  "scripts": {
    "start": "node ./testserver/index.js"
  },
  "dependencies": {
    "chart.js": "2.7.2",
    "cli-progress": "^2.0.0",
